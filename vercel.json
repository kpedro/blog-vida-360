{
  "version": 2,
  "headers": [
    {
      "source": "/assets/images/(.*).png",
      "headers": [{ "key": "Content-Type", "value": "image/png" }]
    }
  ],
  "routes": [
    {
      "src": "/post.html",
      "dest": "/api/dynamic-post"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
