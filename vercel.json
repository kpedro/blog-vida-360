{
  "version": 2,
  "headers": [
    {
      "source": "/assets/images/(.*).png",
      "headers": [{ "key": "Content-Type", "value": "image/png" }]
    }
  ],
  "rewrites": [
    { "source": "/post.html", "destination": "/api/dynamic-post" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
